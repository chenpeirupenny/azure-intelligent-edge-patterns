{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {}
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": "{}"
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "nginxmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/nginxmodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"8181/tcp\":[{\"HostPort\":\"8181\"}]}}}"
            }
          },
          "webmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "TRAINING_KEY": {
                "value": "<Training API Key>"
              },
              "ENDPOINT": {
                "value": "<Training Endpoint>"
              },
              "DBNAME": {
                "value": "vision_on_edge"
              },
              "DBHOST": {
                "value": "172.18.0.1"
              },
              "DBUSER": {
                "value": "vision_on_edge"
              },
              "DBPASS": {
                "value": "vision_on_edge"
              },
              "DF_PD_VIDEO_SOURCE": {
                "value": "opencv"
              }
            },
            "settings": {
              "image": "intelligentedge/webmodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"8000/tcp\":[{\"HostPort\":\"8000\"}]}}}"
            }
          },
          "inferencemodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "IOTHUB_CONNECTION_STRING": {
                "value": "$IOTHUB_CONNECTION_STRING"
              },
              "IS_OPENCV": {
                "value": "true"
              }
            },
            "settings": {
              "image": "intelligentedge/inferencemodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5000/tcp\":[{\"HostPort\":\"5000\"}],\"44000/tcp\":[{\"HostPort\":\"44000\"}],\"5558/tcp\":[{\"HostPort\":\"5558\"}]},\"IpcMode\":\"host\",\"runtime\":\"<Docker Runtime>\"}}"
            }
          },
          "predictmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "IOTHUB_CONNECTION_STRING": {
                "value": "$IOTHUB_CONNECTION_STRING"
              },
              "LVA_MODE": {
                "value": "grpc"
              }
            },
            "settings": {
              "image": "intelligentedge/predictmodule:0.27.2-<cpu or gpu><platform>",
              "createOptions": "{\"HostConfig\":{\"Binds\":[\"/dev/bus/usb:/dev/bus/usb\"],\"DeviceCgroupRules\":[\"c 189:* rmw\"],\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"7777/tcp\":[{\"HostPort\":\"7777\"}]},\"IpcMode\":\"host\",\"runtime\":\"<Docker Runtime>\"}}"
            }
          },
          "cvcapturemodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/cameramodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"9000/tcp\":[{\"HostPort\":\"9000\"}],\"5559/tcp\":[{\"HostPort\":\"5559\"}]},\"runtime\":\"runc\"}}"
            }
          },
          "rtspsim": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/rtspsimmodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"Mounts\":[{\"Target\":\"/live/mediaServer/media/upload\",\"Source\":\"upload_data\",\"Type\":\"volume\"}],\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"554/tcp\":[{\"HostPort\":\"554\"}]}}}"
            }
          },
          "uploadmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/uploadmodule:0.27.2-<platform>",
              "createOptions": "{\"HostConfig\":{\"Mounts\":[{\"Target\":\"/app/upload\",\"Source\":\"upload_data\",\"Type\":\"volume\"}],\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"7000/tcp\":[{\"HostPort\":\"7000\"}]}}}"
            }
          },
          "ovms-server": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/ovms-server:0.27.2-<platform>",
              "createOptions": "{\"ExposedPorts\":{\"9000/tcp\":{}},\"Cmd\":[\"/ovms/bin/ovms --model_path /models/yolov3 --model_name yolov3 --port 9000\"],\"Entrypoint\":[\"sh\",\"-c\"],\"HostConfig\":{\"PortBindings\":{\"9000/tcp\":[{\"HostIp\":\"\",\"HostPort\":\"9000\"}]},\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}}},\"Labels\":{\"com.docker.compose.project\":\"yolov3-ovms-iotedge\",\"com.docker.compose.service\":\"ovms-server\",\"com.docker.compose.oneoff\":\"False\",\"com.docker.compose.container-number\":\"1\",\"com.docker.compose.version\":\"1.24.0\",\"com."
            }
          },
          "ovms-app": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "intelligentedge/ovms-app:0.27.2-<platform>",
              "createOptions": "{\"ExposedPorts\":{\"5010/tcp\":{}},\"Env\":[\"PYTHONUNBUFFERED=1\"],\"HostConfig\":{\"PortBindings\":{\"5010/tcp\":[{\"HostIp\":\"\",\"HostPort\":\"5010\"}]},\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}}},\"Labels\":{\"com.docker.compose.project\":\"yolov3-ovms-iotedge\",\"com.docker.compose.service\":\"ovms-app\",\"com.docker.compose.oneoff\":\"False\",\"com.docker.compose.container-number\":\"1\",\"com.docker.compose.version\":\"1.24.0\",\"com.docker.compose.config-hash\":\"dcdef0702a8e26d7c244eb1fc29534413b8822c98e1db982a604609"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "metrics": "FROM /messages/modules/InferenceModule/outputs/metrics INTO $upstream",
          "InferenceToLVA": "FROM /messages/modules/InferenceModule/outputs/InferenceToLVA INTO BrokeredEndpoint(\"/modules/lvaEdge/inputs/recordingTrigger\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "webmodule": {
      "properties.desired": {}
    }
  }
}